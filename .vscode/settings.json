{
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"app/views/node_modules/**": true,
		"app/views/build/**": true,
	},
	"[javascript]": {
		"editor.detectIndentation": false,
		"editor.tabSize": 2
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",

	"cSpell.words": [
		"briansimoni",
		"gorm",
		"jinzhu",
		"logrus",
		"oauth",
		"octicon",
		"spotify",
		"structs",
		"uuid"
	]
}